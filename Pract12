#### ejercicio clase ####
# calcula que tipo de relacion hay entre x y y, es decir el coeficiente de correlacion muestral r

x<-c(2.4,3.4,4.6,3.7,2.2,3.3,4.0,2.1)
y<-c(1.33,2.12,1.80,1.65,2.00,1.76,2.11,1.63)

#1) calcule r


x2<-x^2
y2<-y^2
xy<-x*y

sxx<-(sum(x2))-(((sum(x))^2)/8)
sxx

syy<-(sum(y2))-(((sum(y))^2)/8)


sxy<-sum(xy)-((sum(x)*sum(y))/8)
sxy

r<-sxy/(sqrt(sxx)*sqrt(syy))
