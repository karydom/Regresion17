#### calcular bo y bi mediante minimos cuadrados ####

x<-c(132,129,120,113.2,105,92,84,83.2,88.4,59,80,81.5,71,69.2)
y<-c(46,48,51,52.1,54,52,59,58.7,61.6,64,61.4,54.6,58.8,58)

x2<-x^2
xy<-x*y

sxx<-(sum(x2))-(((sum(x))^2)/14)
sxx

sxy<-sum(xy)-((sum(x)*sum(y))/14)
sxy

b1<-sxy/sxx
b1

bo<-mean(y)-(b1*mean(x))
bo


y1<-bo+(b1*(115))
y1
y2<-bo+(b1*(138))
y2
y3<-bo+(b1*(55))
y3
y4<-bo+(b1*(99))
y4
y5<-bo+(b1*(290))
y5
y6<-bo+(b1*(71))
y6
